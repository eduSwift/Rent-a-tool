 <div>
  <%= simple_form_for [@select_tool, @select_booking] do |f| %>

    <div class="form-inputs">
     <%= f.input :start_date,
      as: :string,
      input_html: { data: { controller: "datepicker" } } %>
      <%= f.input :end_date,
        as: :string,
       input_html: { data: { controller: "datepicker" } } %>

        <div class="form-actions">
        <%= f.button :submit, class: "btn btn-danger btn-block border border-dark rounded-pill" %>
       </div>
    </div>
 <% end %>
 </div>
